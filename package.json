{
  "name": "patchgate",
  "version": "0.1.3",
  "description": "Policy enforcement and rollback for AI agent code edits",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "patchgate": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build && npm test",
    "dev": "ts-node src/cli.ts",
    "test": "jest --coverage"
  },
  "keywords": [
    "ai",
    "patch",
    "diff",
    "agent",
    "code-safety",
    "llm",
    "patchgate",
    "langchain",
    "langgraph",
    "claude",
    "openai",
    "agent-safety"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shivae372-hub/patchgate.git"
  },
  "homepage": "https://github.com/shivae372-hub/patchgate#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "diff": "^5.1.0",
    "micromatch": "^4.0.7"
  },
  "devDependencies": {
    "@types/diff": "^5.0.9",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  }
}
